<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DivisiApps.Controller.Divisi.DivisiController">
   <children>
      <TableView fx:id="divisiTableView" layoutY="38.0" prefHeight="200.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="kodeDivisiColumn" prefWidth="115.0" text="Kode" />
          <TableColumn fx:id="namaDivisiColumn" prefWidth="320.0" text="Nama" />
            <TableColumn fx:id="kodeKantorColumn" prefWidth="164.0" text="Kode Kantor" />
        </columns>
      </TableView>
      <TextField fx:id="searchText" layoutX="365.0" layoutY="6.0" promptText="Search" />
      <Label layoutX="14.0" layoutY="-1.0" prefHeight="16.0" prefWidth="271.0" text="Daftar Divisi">
         <font>
            <Font name="Arial Black" size="24.0" />
         </font>
      </Label>
      <Button fx:id="btnSearchDivisi" layoutX="545.0" layoutY="5.0" mnemonicParsing="false" onAction="#searchDivisi" text="Cari" />
      <Button fx:id="btnTambahDivisi" layoutX="14.0" layoutY="244.0" mnemonicParsing="false" onAction="#btnTambahDivisi" text="Tambah" />
      <Button fx:id="btnEdit" onAction="#btnEdit" layoutX="97.0" layoutY="244.0" mnemonicParsing="false" text="Edit" />
      <Button fx:id="btnDelete" onAction="#btnDelete" layoutX="150.0" layoutY="244.0" mnemonicParsing="false" text="Delete" />
   </children>
</AnchorPane>
